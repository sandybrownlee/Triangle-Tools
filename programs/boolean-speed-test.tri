let
  var a : Integer;
  var b : Integer
in
begin
  a := 0;
  while a < 5000 do
  begin
    b := 0;
    while b < 3000 do
    begin
      if 1 < 3
      then begin
        if 5 \= 6
          then b++
          else b--
        end
      else b--
    end;
    if 3 < 1
    then a--
    else a++
  end;
end